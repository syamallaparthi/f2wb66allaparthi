extends layout

block content
  h1= title
  - var image_names =  ["image01", "image02", "image03", "image04", "image05"];
  - for(let num=0; num < 3; num++) {
        - let randomValue = Math.floor(Math.random()*5)
        img(src=`/images/${image_names[randomValue]}.jpg`, alt="", height="300px", width="300px")
  - }